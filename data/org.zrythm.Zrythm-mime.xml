<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <!-- see https://docs.oracle.com/cd/E26502_01/html/E28056/gmcas.html for info -->
  <mime-type type="application/x-zrythm-project">
    <comment>Zrythm project</comment>
    <comment xml:lang="fr">Projet Zrythm</comment>
    <glob pattern="*.zpj"/>
    <sub-class-of type="application/zstd"/>
  </mime-type>
  <!-- see #1062 -->
  <mime-type type="application/x-zrythm-package">
    <comment>Zrythm package</comment>
    <glob pattern="*.zpx"/>
    <sub-class-of type="application/zstd"/>
  </mime-type>
</mime-info>
